<Window
    x:Class="ZFileWPFClient.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Nv="clr-namespace:Component.Common;assembly=ZFileComponent"
    xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
  
    xmlns:prism="http://prismlibrary.com/"
    xmlns:zt="clr-namespace:ZFileComponent.Themes.ControlHelper;assembly=ZFileComponent"
    Width="1024"
    Height="768"
    MinWidth="800"
    MinHeight="600"
    prism:ViewModelLocator.AutoWireViewModel="True"
    TextElement.FontSize="14"
    TextElement.FontWeight="Medium"
    TextElement.Foreground="{DynamicResource MaterialDesignBody}">
    <Grid >
        <ContentControl prism:RegionManager.RegionName="{x:Static Nv:SystemResource.Nav_MainContent}" />
        <zt:DialogBox Identifier="{x:Static Nv:SystemResource.Nav_MainContent}" />
    </Grid>


   

</Window>
