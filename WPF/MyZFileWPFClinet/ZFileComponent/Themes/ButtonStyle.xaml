                                    <Setter TargetName="GrdMain" Property="Opacity" Value="{Binding Path=(local:ButtonHelper.ClickCoverOpacity), RelativeSource={RelativeSource AncestorType=Button}, Mode=OneWay}" />
                                </Trigger>
